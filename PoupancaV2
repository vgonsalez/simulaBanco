package br.fundatec.tdp;


public class Poupanca {

	private int idPoupanca;
	private double saldoP;

	
	public Poupanca(double saldoP) {
		this.saldoP = saldoP;
	}

	public int getIdPoupanca() {
		return idPoupanca;
	}

	public void setIdPoupanca(int idPoupanca) {
		this.idPoupanca = idPoupanca;
	}

	public double getSaldoP() {
		return saldoP;
	}

	public void setSaldoP(double saldoP) {
		this.saldoP = saldoP;
	}

}
